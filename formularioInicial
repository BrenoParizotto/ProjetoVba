Sub Cadastrar_Click()
    If IsNumeric(Cliente.Text) Then
        MsgBox "O cliente: " & Cliente.Text & " é inválido.", vbCritical, "Erro"
        Cliente.Text = ""
        Exit Sub
    End If
    
        If IsNumeric(INVESTIDOR.Text) Then
        MsgBox "O representante: " & INVESTIDOR.Text & " é inválido.", vbCritical, "Erro"
        INVESTIDOR.Text = ""
        Exit Sub
    End If
    
      If IsNumeric(ESP.Text) Then
        MsgBox "O especialista: " & ESP.Text & " é inválido.", vbCritical, "Erro"
        ESP.Text = ""
        Exit Sub
    End If
    
    If IsNumeric(Expansao.Text) Then
        MsgBox "O gerente de expansão: " & Expansao.Text & " é inválido.", vbCritical, "Erro"
        Expansao.Text = ""
        Exit Sub
    End If
    
    If IsNumeric(Autorizado.Text) Then
        MsgBox "O autorizado: " & Autorizado.Text & " é inválido.", vbCritical, "Erro"
        Autorizado.Text = ""
        Exit Sub
    End If
    
    If Autorizado.Value = "" Then
    MsgBox "Favor informar nome do autorizado.", vbCritical, ""
    Exit Sub
    End If
    
    If ESP.Value = "" Then
    MsgBox "Favor informar nome do especialista.", vbCritical, ""
    Exit Sub
    End If
    
      If INVESTIDOR.Value = "" Then
    MsgBox "Favor informar nome do representante.", vbCritical, ""
    Exit Sub
    End If
    
    If Expansao.Value = "" Then
    MsgBox "Favor informar nome do gerente de expansão.", vbCritical, ""
    Exit Sub
    End If

    If TextBox5.Value = "" Then
    MsgBox "Favor informar a data de contrato.", vbCritical, ""
    Exit Sub
    End If
    
    If Label9.Visible = False Then
    MsgBox "As parcelas não foram cadastradas.", vbCritical, ""
    Exit Sub
    End If
    
    If CheckBox1.Value = False And CheckBox2.Value = False Then
    MsgBox "Favor marcar a situação do contrato!", vbCritical, ""
    Exit Sub
    End If
    
    If Cliente.Text = "" Then
        MsgBox "Informe o nome do cliente.", vbCritical, "Erro"
        Exit Sub
    End If
    
    If TextBox1 = "" Then
    MsgBox "O número de parcelas está zerado.", vbCritical, ""
    Exit Sub
    End If
    
    If TextBox1 > 10 Then
    MsgBox "Favor verificar o número de parcelas.", vbCritical, ""
    Exit Sub
    End If
    
    If TextBox4 > 100 Or TextBox4 = 0 Then
    MsgBox "Favor verificar a porcentagem do investimento.", vbCritical, ""
    Exit Sub
    End If
    
    If cadastroProvisionamento.UFV.ListIndex < 0 Then
        MsgBox "Não há nenhuma UFV selecionada!", vbCritical, "Aviso"
        Exit Sub
    End If
    
    Dim resposta As Integer
    resposta = MsgBox("Confirma o cadastro de pagamento?", vbInformation + vbYesNo, "Confirmação")
    If resposta = vbNo Then Exit Sub
    
    Call cadastrarProv
   
    cadastroProvisionamento.UFV.ListIndex = -1
End Sub

Sub cadastrarProv()
    Dim ws As Worksheet
    Dim i As Long
    Set ws = ThisWorkbook.Sheets("BasePagamentos")
    Dim lastRow As Long
    
    lastRow = ws.Cells(ws.Rows.Count, 1).End(xlUp).Row + 1
    
    If TextBox1.Value = 1 Then
    ws.Cells(lastRow, 1).Value = UCase(Cliente.Text) 'investidor
    ws.Cells(lastRow, 2).Value = UCase(ESP.Text) 'especialista
    ws.Cells(lastRow, 3).Value = TextBox5.Value 'dtcontrato
    ws.Cells(lastRow, 4).Value = UFV.Value 'ufv
    ws.Cells(lastRow, 6).Value = TextBox3.Value 'vlrtotal
    ws.Cells(lastRow, 14).Value = UCase(Expansao.Text)
    ws.Cells(lastRow, 15).Value = UCase(Autorizado.Text)
