    ws.Cells(lastRow, 21).Value = TextBox4.Value / 100 'vlrtotal
If CheckBox1.Value = True Then
ws.Cells(lastRow, 17).Value = "ASSINADO"
Else
ws.Cells(lastRow, 17).Value = "AGUARDANDO ASSINATURA"
End If
    
